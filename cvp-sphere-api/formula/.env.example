# Database Connection Configuration
# Copy this file to .env and fill in your actual credentials
# NEVER commit .env file to version control

# Required: Database username
DB_USER=MATH_USER

# Required: Database password  
DB_PASSWORD=########$

# Optional: Database host (default: 172.169.88.80)
DB_HOST=172.169.88.80

# Optional: Database port (default: 1521)
DB_PORT=1521

# Optional: Database SID (default: DEV)
DB_SID=DEV

# Note: For production, use more secure methods like:
# - Kubernetes secrets
# - AWS Secrets Manager
# - HashiCorp Vault
# - Environment variables in CI/CD pipeline
